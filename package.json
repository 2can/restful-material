{
  "name": "restful-material",
  "version": "0.1.0",
  "description": "",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git@github.com:asok/restful-material.git"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "nodemon -w __tests__ -w ./src -e js,jsx -x npm test -i *flycheck_*",
    "prebuild": "rm -rf lib",
    "build": "./node_modules/.bin/babel --stage 1 ./src --out-dir ./lib",
    "build:watch": "nodemon -w ./src -e js,jsx -x npm run build -i *flycheck_*"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/*"
    ]
  },
  "keywords": [
    "framework",
    "materialui",
    "react",
    "rest"
  ],
  "author": "asok",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asok/restful-material/issues"
  },
  "homepage": "https://github.com/asok/restful-material",
  "devDependencies": {
    "babel-jest": "~4.0.0",
    "fakexmlhttprequest": "0.0.1",
    "jest-cli": "^0.4.2",
    "native-promise-only": "^0.7.8-a",
    "nodemon": "^1.3.7",
    "pprint": "0.0.1"
  },
  "dependencies": {
    "babel": "^5.4.3",
    "react": "~0.13.0",
    "react-intl": "~1.2.0",
    "react-tap-event-plugin": "~0.1.4",
    "material-ui": "~0.7.5",
    "underscore": "~1.8.3"
  }
}
